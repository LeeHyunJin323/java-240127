package j07_반복;

public class For1 {

	public static void main(String[] args) {
//	
//		for (int i = 0 ; i<5; i++) {
//			System.out.println(i);
//		
//		}
//		
		System.out.println("for 후");
		/*
		 * <for문의 기본형태>
		 * for(초기치;조건문;증가치){
		 * 
		 *     실행할 문장 a;
		 *     실행할 문장 b;
		 *     ...
		 */
		
		// for로 구구단 만들기
		
		for(int i = 2 ; i<10; i++) {
			System.out.println(i+"단");
			System.out.println();
			for (int j=1; j<10; j++)
			{
				System.out.println(i + "X " + j +" = "+ i*j);
			}
		}
	}

}
