package j07_반복;

public class For3 {

	public static void main(String[] args) {
	   /*
	    * str = abcdefghijk
	    * 
	    * 반복문을 돌려서 출력하시오
	    */
		
		String str = "abcdefhijk";
		int j=0;
	     for(int i = 0; i<str.length(); i++) {
	    	 System.out.println(str.substring(i,j+1));
	    	 
	     }
	     
	    
	    
	   
		

	}

}
