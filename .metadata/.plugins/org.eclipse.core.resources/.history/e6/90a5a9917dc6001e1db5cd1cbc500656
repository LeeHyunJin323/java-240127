package j08_메소드;




public class Method1 {

	
	public int add(int n1 , int n2)
	{
		int result = n1+n2;
		return result;
	}
	
	
	public static void main(String[] args) {
	
		 int num1 = 10;
		 int num2 = 20;
		 Method1 method1 = new Method1();
		 
		 
	
		int sum =  method1.add(num1,num2);
		
		 System.out.println(sum);
		 
		 
	}
	
	
	}

	

