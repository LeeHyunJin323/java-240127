package j07_반복;

public class DoubleWhile2 {

	public static void main(String[] args) {

		int i = 2;
		int j = 1;
		while (i < 9) {
			j = 1;
			
			System.out.println(i+"단");
			System.out.println();
			while (j < 9) {
				
				System.out.println(i+"X"+j+ "="+j*i);
				j++;
			}
			
			i++;
			
		}

	
	}

}
