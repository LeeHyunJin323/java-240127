package j03_연산자;

public class Operation5 {

	public static void main(String[] args) {
		String str = "안";
		System.out.println(str.length());
		
		
		String result;
		String pw = 1q2q3err4;
		
		
		result = (pw.length() != 8 || pw.length() != 16)?"8~16 자리를 충족하면 비밀번호가 확인되어습니다."
				:"비밀번호가 8~16자리가 되어야함.";
		
		
		
		/*
		 * pw
		 * 비밀번호가 8~16자리가 되어야함.
		 * 8~16 자리를 충족하면 비밀번호가 확인되어습니다.
		 * 비밀번호자릿수를 맞춰주세요
		 * 비밀번호 미작성 시 비밀번호를 입력하세요.
		 */

	}

}
