package j07_반복;

public class DoubleWhile2 {

	public static void main(String[] args) {

		int i = 1;
		int j = 0;
		while (i < 9) {
			j = 1;
			
			System.out.println();
			System.out.println(i+"단");
			System.out.println();
			while (j < 9) {
				
				System.out.println((i+1)+"X"+(j+1)+ "="+(j+1)*(i+1));
				j++;
			}
			i++;
		}

	
	}

}
