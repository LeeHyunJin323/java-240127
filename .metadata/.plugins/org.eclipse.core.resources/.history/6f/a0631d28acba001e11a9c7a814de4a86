package j06_조건;

public class If2_2 {

	public static void main(String[] args) {
		/*
		 * 학점 판독기
		 * 점수(score)
		 * 
		 * 점수가 89점 초과 A학점 입니다.
		 * 점수가 79점 초과 B학점 입니다.
		 * 점수가 69점 초과 C학점 입니다.
		 * 점수가 59점 초과 D학점 입니다.
		 * 점수가 59점 이하 F학점 입니다.
		 * 점수가 0점이하이거나 100점 초과이면 -> 계산불가
		 * 
		 */
		int score = 59;
		
		if(score <= 0 && score >= 100) {
			System.out.println("계산불가");
		}

	}

}
